{
  "manifest_version": 3,
  "name": "Ariadne",
  "description": "helps you to record your web journey",
  "version": "1.0",
  "chrome_url_overrides": {
    "history": "history.html"
  },
  "permissions": ["history", "favicon", "storage"],
  "icons": {
    "16": "images/history16.png",
    "32": "images/history32.png",
    "128": "images/history128.png"
  }
}
